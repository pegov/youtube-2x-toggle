{
  "manifest_version": 2,
  "name": "Youtube 2x toggle",
  "version": "1.0.3",
  "description": "Add 1x/2x playback speed toggle to the Youtube video player bottom right panel.",
  "homepage_url": "https://github.com/pegov/youtube-2x-toggle",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": [
    "images/*.svg"
  ]
}
